<!DOCTYPE html>
<html>
	<head>
  		<title><%= full_title(yield(:title)) %></title>
  		<%= stylesheet_link_tag    "application", media: "all" %>
  		<%= javascript_include_tag "application" %>
  		<%= csrf_meta_tags %>
	</head>
    <body>
        <% if current_user %>
          <%= link_to "Sign Out", destroy_user_session_path %>
        <% else %>
          <%= link_to "Login", new_user_session_path %>
        <% end %>
		<%= yield %>
	</body>
</html>
